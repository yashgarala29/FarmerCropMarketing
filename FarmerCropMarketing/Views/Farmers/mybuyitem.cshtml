@model IEnumerable<FarmerCropMarketing.Models.Orders>

@{
    ViewData["Title"] = "mybuyitem";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>mybuyitem</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Farmers.Farmers_address)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Crops.Crops_description)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.seller_id)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.order_date)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.order_status)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.delivery_date)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.total_pricer)
            </th>
           
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Farmers.Farmers_address)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Crops.Crops_description)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.seller_id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.order_date)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.order_status)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.delivery_date)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.total_pricer)
            </td>
          
        </tr>
}
    </tbody>
</table>
