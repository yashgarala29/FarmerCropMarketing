@model IEnumerable<FarmerCropMarketing.Models.Class.MSPSellCrops>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Farmers_name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Farmers_email)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Farmers_mobile_no)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Farmers_address)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MSPCropsDetail)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Farmers_name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Farmers_email)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Farmers_mobile_no)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Farmers_address)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MSPCropsDetail.Crops_description)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.MSPSell_id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.MSPSell_id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.MSPSell_id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
